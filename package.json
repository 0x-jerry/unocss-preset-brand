{
  "name": "@0x-jerry/unocss-preset-brand",
  "version": "0.1.2",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./runtime": {
      "default": "./src/runtime.ts"
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsup src/index.ts src/runtime.ts --format esm --dts --clean",
    "example:dev": "npm run -C example dev",
    "example:build": "npm run -C example build",
    "prepublishOnly": "npm run build",
    "release": "x-release"
  },
  "license": "MIT",
  "author": "0x-jerry <x.jerry.wang@gmail.com>",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./runtime": {
        "default": "./dist/runtime.js",
        "types": "./dist/runtime.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@0x-jerry/x-release": "^1.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.5.0",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@0x-jerry/utils": "^2.1.2",
    "@unocss/core": "^0.59.2",
    "lodash-es": "^4.17.21"
  }
}
